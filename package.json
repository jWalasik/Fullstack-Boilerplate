{
  "name": "apollo-boilerplate",
  "version": "1.0.0",
  "description": "Fullstack boilerplate based on Apollo platform",
  "scripts": {
    "build": "yarn run webpack -p --config=config/prod.js",
    "build-server": "tsc",
    "lint": "tslint --format stylish --project . --force",
    "start-client": "run start-dev",
    "start-server": "nodemon --exec babel-node ./server/server.ts",
    "start-dev": "webpack-dev-server --hot --content-base src --inline --config=config/dev.js --public --env.ENVIRONMENT=dev",
    "start-prod": "yarn run build && node express.js",
    "start": "concurrently \"yarn start-dev\" \"yarn start-server\"",
    "test": "jest"
  },
  "keywords": [
    "React",
    "Apollo",
    "MongoDB",
    "Typescript",
    "GraphQL"
  ],
  "author": "Jacek Walasik",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.25.4",
    "apollo-server-express": "^2.10.1",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "^14.6.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mongoose": "^5.8.11",
    "nodemailer": "^6.4.6",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "react": "^16.12.0",
    "react-apollo": "^3.1.4",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.33",
    "@types/react-dom": "^16.9.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "chalk": "^3.0.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.2",
    "react-hot-loader": "^4.12.18",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
